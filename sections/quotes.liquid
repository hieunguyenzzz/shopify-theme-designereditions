<link rel="stylesheet" href="https://npmcdn.com/flickity@2/dist/flickity.css">
 <div class="page-width" data-section-id="{{ section.id }}" data-section-type="quotes">
  {% if section.settings.title != blank %}
    <div class="section-title row">
      <h1>{{ section.settings.text1 | escape }}</h1>
         <p>{{ section.settings.title | escape }}</p>
    </div>
  {% endif %}

  {% if section.blocks.size > 0 %}
    <div class="quotes-wrapper" data-slider>
      <div class="quotes-slider-wrapper" data-slider-container>
        <div class="main-carousel" id="Quotes-{{ section.id }}" data-flickity='{ "contain": true,"groupCells": 3,"wrapAround": false, "prevNextButtons": true,"adaptiveHeight": true
 }'>
          {% for block in section.blocks %}
            <div id="slickSlide{{ block.id }}"
              class="carousel-cell quotes-slide quotes-slide--{{ block.id }}{% if section.blocks.size == 1 %} quotes-slide--single{% endif %}"
              aria-labelledby="slickDot{{ block.id }}"
              {{ block.shopify_attributes }}
              data-slider-slide-index="{{ forloop.index0 }}"
              data-slider-item>

              <blockquote class="text-center quotes-slider__text">
                <span class="quote-icon">
                 <img class="b-review-avatar lazyloaded" src="{{ block.settings.image | img_url: '200x' }}" >
                
                </span>
                
                <div class="b-review-stars"> 
                  <img src="https://cdn.shopify.com/s/files/1/0563/0938/3354/files/Screenshot_12.png?v=1618902716">
                </div>
                <h5>{{ block.settings.title1 }}
                </h5>
                {% if block.settings.quote != blank %}
                  <div class="rte-setting rte">{{ block.settings.quote }}</div>
                {% endif %}
                {% if block.settings.author != blank %}
                  <cite>{{ block.settings.author | escape }}</cite>
                {% endif %}
              </blockquote>
            </div>
          {% endfor %}
        </div>
      </div>

 
    </div>
  {% endif %}

  {% if section.blocks.size == 0 %}
    {% include 'no-blocks' %}
  {% endif %}
</div>

<style>  
    @media screen and (min-width: 769px)
    { 
.quotes-slider-wrapper .flickity-slider {
    position: absolute;
   
}
 
 .b-review-stars img {
    width: 80px;
    margin: 0;
}blockquote {
    padding: 29px 6px;
    border: 1px solid #00000059;
  width:385px;
} 
.quotes-slider-wrapper .review-stars{
  font-size: 15px;
}
    }
</style>


{% for designer in designers %}
    {% assign handlizedDisigner = designer | handleize %}
    {% assign collection = collections[handlizedDisigner] %}
    {% if collection and collection.products.size > 0  %}
        <li {% if class %} class="{{class}}" {% else %} class="megamenu__listitem tw-w-[340px]" {% endif %}>
            <a {% if class %} {% else %} class="megamenu__listlink" {% endif %} href="/collections/{{designer | handleize}}">{{designer}}</a>
        </li>
    {% endif %}
{% endfor %}
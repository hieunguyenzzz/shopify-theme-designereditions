@font-face {
  font-family: "DIN Next LT W04 Medium";
  src: url('{{ 'DIN-Next-LT-W04-Medium.eot' | asset_url }}');
  src: url('{{ '"DIN-Next-LT-W04-Medium.eot?#iefix"' | asset_url }}')
      format("embedded-opentype"),
    url('{{ 'DIN-Next-LT-W04-Medium.woff2' | asset_url }}') format("woff2"),
    url('{{ 'DIN-Next-LT-W04-Medium.woff' | asset_url }}') format("woff"),
    url('{{ 'DIN-Next-LT-W04-Medium.ttf' | asset_url }}') format("truetype"),
    url("{{ 'DIN-Next-LT-W04-Medium.svg#DINNextLTW04-Medium' | asset_url }}") format("svg");
}

